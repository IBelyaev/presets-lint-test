{
  "name": "presets-lint-test",
  "version": "0.0.0-development",
  "description": "This project  only for test new CI flow, all code base inside is copy from https://github.com/alfa-laboratory/arui-presets-lint",
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/github": "^7.1.1",
    "arui-cssvars": "1.1.7",
    "babel-eslint": "^10.0.3",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "command-line-args": "5.1.1",
    "conventional-changelog-cli": "1.3.21",
    "conventional-github-releaser": "2.0.2",
    "react": "16.13.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "stylelint": "^12.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "7.0.3",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "husky": "3.1.0",
    "lint-staged": "9.4.2",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "semantic-release": "^17.1.1",
    "stylelint": "12.0.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint ./",
    "test": "yarn test:eslint && yarn test:stylelint",
    "test:eslint": "eslint \"./test/**/*.{ts,tsx,js,jsx}\"",
    "test:stylelint": "node ./test/stylelint-test.js",
    "format": "prettier-eslint --write \"./{eslint,stylelint,test,commitlint}/**/*.{ts,tsx,js,jsx,css}\"",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css}": [
      "prettier-eslint --write",
      "eslint"
    ],
    "stylelint/*.js": [
      "prettier-eslint --write",
      "yarn test:stylelint"
    ]
  },
  "resolutions": {
    "set-value": ">=2.0.1",
    "js-yaml": ">=3.13.1",
    "braces": ">=2.3.1",
    "lodash.merge": ">=4.6.2",
    "lodash": ">=4.17.12",
    "mixin-deep": ">=1.3.2",
    "handlebars": ">=4.5.3",
    "kind-of": ">= 6.0.3",
    "minimist": ">= 1.2.3"
  },
  "prettier": "./prettier.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBelyaev/presets-lint-test.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
